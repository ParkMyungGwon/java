
public class ArithOp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n1 = 10;
		int n2 = 3;
		
		int result = n1 + n2;
		
		System.out.println("³ª´°¼À °á°ú" + n1 / n2);
		
		System.out.println("³ª¸ÓÁö °á°ú" + n1 % n2);
		
		
		int a = 7 ;
		
		a = ~a;
		a++;
		System.out.println(a);
		
		int b= 15678;
		
		b = b >> 2;
		
		System.out.println(b);
		
	}

}
