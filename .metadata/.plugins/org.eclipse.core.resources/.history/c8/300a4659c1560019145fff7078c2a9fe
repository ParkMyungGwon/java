interface UpperCasePrintable{
	
}

class PointOne implements UpperCasePrintable{
	private int xpos, ypos;
	
	PointOne(int x, int y){
		xPos = x;
		yPos = y;
	}
	
	public String toString(){
		String posInfo = "{x pos :"+ xpos + ", y pos : "+ypos+"}";
	}
}

