
public class Constructor1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Number num1 = new Number();
		System.out.println(num1.getNumber());
	}

}

class Number{
	
	int num;
	
	public Number(){
		
		num = 20;
		System.out.println("생성자 호출");
	}
	
	public int getNumber(){
	
		return num;
	}
}
