
public class ClassVar {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		InstCnt cnt1 = new InstCnt();
		InstCnt cnt2= new InstCnt();
		InstCnt cnt3= new InstCnt();
	}

}

class InstCnt{
	
	static int cnt = 0;

	public InstCnt(){
		cnt++;
		System.out.println(cnt);
	}
}
