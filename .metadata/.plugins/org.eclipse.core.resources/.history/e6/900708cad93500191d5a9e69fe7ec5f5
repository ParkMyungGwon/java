
public class SwitchBreak {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=3;
		
		switch(n){
		
		case 1 : 
			System.out.println("1");
			
		case 2 :
			System.out.println("2");
			break;
		case 3 :
			System.out.println("3");
			break;
		
		}
		
		
	}

}
