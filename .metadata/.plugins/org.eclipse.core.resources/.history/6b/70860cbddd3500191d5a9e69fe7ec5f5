
public class BreakBasic {

	
	public static void main(String[] args){
		
		int num = 1;
		int cnt = 0;
		while(true){
			
			if(num % 2 != 0  || (num / 3 == 0 && num / 2 == 0)){
				cnt += num;
				System.out.println(num);
			}
			
			
			if(num > 1000)
				break;
			else
				num++;
			
		}
			
			
		}
		
	}

