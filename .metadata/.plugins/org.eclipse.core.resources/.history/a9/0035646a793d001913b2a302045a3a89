
public class ClassVarAccess {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		AccessWay way = new AccessWay();
		way.num++;
	}

}

class AccessWay{
	
	static int num = 0;
	
	public AccessWay(){
		incrCnt();
	}
	
	public void incrCnt(){
		num++;
		System.out.println("나실행됬니???");
		System.out.println("incrCnt : " + num);
	}
	
}
