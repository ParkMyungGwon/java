
public class ArithOp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n1 = 10;
		int n2 = 3;
		
		int result = n1 + n2;
		
		System.out.println("나눗셈 결과" + n1 / n2);
		
		System.out.println("나머지 결과" + n1 % n2);
		
		
		int a = 7 ;
		
		a = ~a;
		System.out.println(a);
	}

}
